<h1>Hello, Rails!</h1>

<ul>
    <% @articles.each do |article| %>
        <li>
            <%= article.title%>
        </li>
    <% end %>
</ul>

<h1>Articles</h1>

<ul>
  <% @articles.each do |article| %>
    <li>
      <%= link_to article.title, article %>
    </li>
  <% end %>
</ul>


<li><%= link_to "New Article", new_article_path(@article) %></li>